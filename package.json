{
  "publisher": "codegraphy",
  "name": "codegraphy-js-plugin",
  "displayName": "CodeGraphy JS Plugin",
  "version": "0.0.1",
  "description": "",
  "categories": [
    "Other"
  ],
  "main": "./dist/extension.js",
  "engines": {
    "vscode": "^1.69.0"
  },
  "activationEvents": [
    "*"
  ],
  "contributes": {},
  "scripts": {
    "vscode:prepublish": "npm run compile",
    "compile": "concurrently \"tsc -p ./tsconfig.extension.json --skipLibCheck\" \"vite build\"",
    "watch": "concurrently \"tsc -w -p ./tsconfig.extension.json --skipLibCheck\" \"vite build --watch\"",
    "lint": "eslint .",
    "format": "eslint . --fix",
    "typecheck": "tsc --noEmit --skipLibCheck",
    "test": "vitest --passWithNoTests",
    "test:ui": "vitest --ui"
  },
  "dependencies": {
    "@vitest/ui": "^0.26.3",
    "vis-data": "^7.1.6",
    "vis-network": "^9.1.6"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^0.38.6",
    "@testing-library/jest-dom": "^5.16.5",
    "@testing-library/user-event": "^14.4.3",
    "@types/jest": "^29.5.1",
    "@types/node": "^18.16.9",
    "@types/vscode": "^1.78.0",
    "concurrently": "^7.6.0",
    "eslint": "^8.40.0",
    "jsdom": "^20.0.3",
    "typescript": "^4.9.5",
    "vite": "^2.9.15",
    "vitest": "^0.26.3"
  }
}
